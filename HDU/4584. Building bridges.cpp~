#include <iostream>
#include <cstdio>
#include <cmath>
using namespace std;

char graph[44][44];
int main()
{
	int m, n;
	while(cin >> n >> m, m || n)
	{
		for(int i = 0;i < n;i++)
			scanf("%s", graph[i]);

		int huX, huY, cuX, cuY;
		int dist = -1;
		for(int i = 0;i < n;i++)
			for(int j = 0;j < m;j++)
			{
				if (graph[i][j] != 'H')
					continue;
				for(int k = 0;k < n;k++)
					for(int l = 0;l < m;l++)
					{
						if (graph[k][l] != 'C')
							continue;

						if (abs(k - i) + abs(j - l) < dist || dist == -1)
						{
							huX = i, huY = j;
							cuX = k, cuY = l;
							dist = abs(k - i) + abs(j - l);
						}
					}
			}
		printf("%d %d %d %d\n", huX, huY, cuX, cuY);
	}
	return 0;
}
