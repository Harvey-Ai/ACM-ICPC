#include <iostream>
#include <set>
using namespace std;

int main()
{
	int n;

	int arr[105];
	while(cin >> n)
	{
		set<int> sumSet;

		for(int i = 0;i < n;i++)
			cin >> arr[i];

		for(int i = 0;i < n;i++)
			for(int j = i + 1;j < n;j++)
				sumSet.insert(arr[i] + arr[j]);

		__int64 res = 0;
		for(set<int>::iterator it = sumSet.begin();it != sumSet.end();it++)
			res += (__int64)(*it);
		cout << res << endl;
	}
	return 0;
}
