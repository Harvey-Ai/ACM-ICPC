#include <iostream>
#include <cstring>
using namespace std;

int arrV1[105], arrV2[105];
int main()
{
	int f, v;
	cin >> f >> v;

	memset(arrV1, 0, sizeof(arrV1));
	memset(arrV2, 0, sizeof(arrV2));

	for(int i = 0;i < f;i++)
	{
		int maxPre = arrV1[i];
		for(int j = 0;j < v;j++)
		{
			int a;
			cin >> a;
			
            if (j < i)
                continue;
            
			arrV2[j + 1] = a + maxPre;
			if (arrV1[j + 1] > maxPre)
				maxPre = arrV1[j + 1];
		}

		for(int j = 0;j <= v;j++)
			arrV1[j] = arrV2[j];
	}
	
	int maxV = -1000000;
	for(int i = 1;i <= v;i++)
		if (maxV < arrV1[i])
			maxV = arrV1[i];
	cout << maxV << endl;

	return 0;
}
