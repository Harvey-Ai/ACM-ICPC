#include <stdio.h>
#include <string.h>
#define N 1000
#define MAX(a, b) ((a) > (b) ? (a) : (b))

int dp[N][N];
int main()
{
	char s1[N], s2[N];
	int i, j, l1, l2, temp;

	while(scanf("%s", s1) != EOF)
	{
		scanf("%s", s2);

		l1 = strlen(s1);
		l2 = strlen(s2);

		memset(dp, 0, sizeof(dp));

		for(i = 1;i <= l1;i++)
			for(j = 1;j <= l2;j++)
			{
				if (s1[i -1] == s2[j - 1])
					temp = dp[i - 1][j - 1] + 1;
				else
					temp = dp[i - 1][j - 1];

				temp = MAX(temp, MAX(dp[i - 1][j], dp[i][j - 1]));
				dp[i][j] = temp;
			}

		printf("%d\n", dp[l1][l2]);
	}
	return 0;
}

