#include <iostream>
#include <cstring>
using namespace std;

long long dp[50];
int main()
{
	int ncase;

	cin >> ncase;
	for(int icase = 1;icase <= ncase;icase++)
	{
		int n;
		cin >> n;

		memset(dp, 0, sizeof(dp));
		dp[0] = 1;
		dp[1] = 1;
		for(int i = 2;i <= n;i++)
			for(int j = i - 2;j >= 0;j--)
				dp[i] += dp[j];
		
		long long res = 0;
		for(int i = 0;i <= n;i++)
			res += dp[i];

		cout << "Scenario #" << icase << ":" << endl;
		cout << res << endl;
        cout << endl;
	}
	return 0;
}
