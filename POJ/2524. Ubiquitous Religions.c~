#include <stdio.h>
#include <string.h>

int students[50005];
int getF(int id)
{
	int father = id;
	while(students[id] >= 0)
	{
		id = students[id];
		father = id;
	}
	return father;
}

void uion(int id1, int id2)
{
	int fA, fB;
	fA = getF(id1);
	fB = getF(id2);

	if (fA == fB)
		return;
	if (students[fA] < students[fB])
	{
		students[fA] += students[fB];
		students[fB] = fA;
	}
	else
	{
		students[fB] += students[fA];
		students[fA] = fB;
	}
}
int main()
{
	int i, a, b, m, n, res, ncase = 1;

	while(scanf("%d %d", &n, &m), m || n)
	{
		memset(students, -1, sizeof(students));

		for(i = 0;i < m;i++)
		{
			scanf("%d %d", &a, &b);
			uion(a, b);
		}

		res = 0;
		for(i = 1;i <= n;i++)
			if(students[i] < 0)
				res++;
		printf("Case %d: %d\n", ncase++, res);
	}
	return 0;
}
