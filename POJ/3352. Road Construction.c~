#include <stdio.h>
#include <string.h>
#define N 1005

int g[N][N];
int visit[N];
int dp[N];
int depth[N];

void dfs(int k,int dep, int n)
{
	int i;
	if (!visit[k])
	{
		dp[k] = dep;
		depth[k] = dep;
		visit[k] = 1;
	}

	visit[k] = 1;

	for(i = 1;i <= n;i++)
	{
		if (g[k][i])
		{
			if (!visit[i])
				dfs(i, k + 1, n);
			
			if (dp[k] > dp[i])
				dp[k] = dp[i];

			if (dp[k] >= depth[k])
				res++;
		}
	}
}

int main()
{
	int n, r;
	int w, v;
	scanf("%d %d", &n, &r);
	memset(g, 0, sizeof(g));
	while(r--)
	{
		scanf("%d %d", &v, &w);
		g[v][w] = g[w][v] = 1;
	}

	res = 0;
	memset(dp, 0, sizeof(dp));
	memset(visit, 0, sizeof(visit));
	memset(depth, 0, sizeof(depth));
	dfs(1, 1, n);
	printf("%d\n", res);
	return 0;
}
