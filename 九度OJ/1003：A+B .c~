#include <iostream>
using namespace std;

int main()
{
	char str1[100], str2[100], ss1[100], ss2[100];
	int i, j;

	while(cin >> str1 >> str2)
	{
		for(i = 0, j = 0;str1[i] != '\0';i++)
			if (str1[i] >= '0' && str1[i] <= '9' || str1[i] == '-')
				ss1[j++] = str1[i];
		ss1[j] = '\0';

		for(i = 0, j = 0;str2[i] != '\0';i++)
			if (str2[i] >= '0' && str2[i] <= '9'|| str2[i] == '-')
				ss2[j++] = str2[i];
		ss2[j] = '\0';

		int flag1,  flag2;
		flag1 = flag2 = 0;
		int num1 = 0;
		i = 0;
		if (ss1[i] == '-')
		{
			flag1 = 1;
			i++;
		}
		for(;ss1[i] != '\0';i++)
		{
			num1 *= 10;
			num1 += ss1[i] - '0';
		}

		int num2 = 0;
		i = 0;
		if (ss2[i] == '-')
		{
			flag2 = 1;
			i++;
		}
		for(;ss2[i] != '\0';i++)
		{
			num2 *= 10;
			num2 += ss2[i] - '0';
		}
		if (flag1) num1 = -num1;
		if (flag2) num2 = -num2;
		cout << num1 + num2 << endl;
	}
	return 0;
}
