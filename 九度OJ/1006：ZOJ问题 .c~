#include <iostream>
#include <cstring>
using namespace std;


int check(char *str)
{
	if (strcmp(str, "zoj") == 0)
		return 1;
	
	int i, j;
	int midZ;
	int a, b;
	char ss[1005];
	int len = strlen(str);

	for(i = 0, a = 0;i < len && str[i] == 'o';i++)
		a++;
	for(j = len - 1, b = 0;j >= 0 && str[j] == 'o';j--)
		b++;

	if (a == b && len == a + b + 3 && str[a] == 'z' && str[len - b - 1] == 'j' && str[a + 1] == 'o')
		return 1;

	if (b >= a && str[a] == 'z' && str[len - b - 1] == 'j')
	{
		midZ = 0;
		for(i = a + 1;i < len - b - 1;i++)
			if (str[i] != 'o')
				return 0;
			else
				midZ++;

		if (midZ < 1)
			return 0;

		for(i = 0;i < a;i++)
			ss[i] = str[i];

		midZ--;
		ss[i++] = 'z';
		for(;midZ;midZ--, i++)
			ss[i] = 'o';
		ss[i++] = 'j';
		b -= a;
		for(;b;b--,i++)
			ss[i] = 'o';
		ss[i] = '\0';
		return check(ss);
	}
	return 0;
}

int main()
{
	char str[1005];

	while(cin >> str)
	{
		if (check(str))
			cout << "Accepted" << endl;
		else
			cout << "Wrong Answer" << endl;
	}
	return 0;
}
