#include <iostream>
#include <cstring>
using namespace std;


int convert(char *str)
{
	if (strcmp(str, "zero") == 0)
		return 0;
	else if (strcmp(str, "one") == 0)
		return 1;
	else if (strcmp(str, "two") == 0)
		return 2;
	else if (strcmp(str, "three") == 0)
		return 3;
	else if (strcmp(str, "four") == 0)
		return 4;
	else if (strcmp(str, "five") == 0)
		return 5;
	else if (strcmp(str, "six") == 0)
		return 6;
	else if (strcmp(str, "seven") == 0)
		return 7;
	else if (strcmp(str, "eight") == 0)
		return 8;
	else if (strcmp(str, "nine") == 0)
		return 9;
}
int main()
{
	char str[100];

	while(cin >> str)
	{
		int a = convert(str);
		cin >> str;
		while(str[0] != '+')
		{
			a *= 10;
			a += convert(str);
			cin >> str;
		}

		cin >> str;
		int b = convert(str);
		cin >> str;
		while(str[0] != '=')
		{
			b *= 10;
			b += convert(str);
			cin >> str;
		}
		if (a == 0 && b == 0)
			break;
		else
			cout << a + b << endl;
	}
	return 0;
}


