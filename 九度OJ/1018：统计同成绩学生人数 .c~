#include <iostream>
using namespace std;

int main()
{
	int n;
	int i;
	int arr[10000];
	int a;

	while(cin >> n, n)
	{
		for(i = 0;i < n;i++)
			cin >> arr[i];
		cin >> a;
		int res = 0;
		for(i = 0;i < n;i++)
			if (a == arr[i])
				res++;
		cout << res << endl;
	}
	return 0;
}
