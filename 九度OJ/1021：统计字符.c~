#include <iostream>
using namespace std;

int main()
{
	char str[1000], ss[1000];
	int i, j;
	while(cin.getline(str, 1000, '\n'))
	{
		if (str[0] == '#')
			break;
		cin.getline(ss, 1000, '\n');
		
		for(i = 0;str[i] != '\0';i++)
		{
			int cnt = 0;
			for(j = 0;ss[j] != '\0';j++)
				if (str[i] == ss[j])
					cnt++;
			cout << str[i] << " " << cnt << endl;
		}
	}
	return 0;
}
