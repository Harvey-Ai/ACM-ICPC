#include <iostream>
#include <cstring>
using namespace std;

struct tt
{
	char begin[100], end[100];
};

int sub(char *s1, char *s2)
{
	int hour = (s2[0] - s1[0]) * 10 + s2[1] - s1[1];
	int minute = (s2[3] - s1[3]) * 10 + s2[4] - s1[4];
	return hour * 60 + minute;
}

int main()
{
	struct tt boat[1000];
	int flag[1000];
	int n;
	char c;
	char ttime[1000];
	int i;

	while(cin >> n, n != -1)
	{
		memset(flag, 0, sizeof(flag));
		cin >> c;
		cin >> ttime;
		if (c == 'S')
		{
			strcpy(boat[n].begin, ttime);
			flag[n] = 1;
		}
		else
		{
			strcpy(boat[n].end, ttime);
			flag[n] = 2;
		}
		if (n == 0)
		{
			cout << 0 << " " << 0 << endl;
			continue;
		}

		while(cin >> n)
		{
			cin >> c;
			cin >> ttime;
			if (c == 'S')
			{
				strcpy(boat[n].begin, ttime);
				flag[n] |= 1;
			}
			else
			{
				strcpy(boat[n].end, ttime);
				flag[n] |= 2;
			}
			if (!n)
				break;
		}

		int res1, res2;
		res1 = res2 = 0;
		for(i = 1;i < 101;i++)
			if (flag[i] == 3)
			{
				res1++;
				res2 += sub(boat[i].begin, boat[i].end);
			}
		if (res1 == 0)
			cout << "0 0" << endl;
		else
		{
			if ((res2 * 1.0 / res1) - (res2 / res1) >= 0.5)
				res2 = res2 / res1 + 1;
			else
				res2 = res2 / res1;
			cout << res1 << " " << res2 << endl;
		}
	}
	return 0;
}

