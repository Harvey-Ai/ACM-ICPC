#include <iostream>
using namespace std;

int main()
{
	int i, j;
	char str[10000];

	while(cin >> str, str[0] != 'E')
	{
		int arr[3] = {0, 0, 0};
		char ss[3] = {'Z', 'O', 'J'};

	//	arr[0] = arr[1] = arr[2] = 0;
		for(i = 0;str[i] != '\0';i++)
		{
			if (str[i] == 'Z')
				arr[0]++;
			else if (str[i] == 'O')
				arr[1]++;
			else arr[2]++;
		}

		//cout << arr[0] << arr[1] << arr[2] << endl;
		for(i = 0, j = 0;str[i] != '\0';i++)
		{
			while (arr[j] == 0)
				j = (j + 1) % 3;
			cout << ss[j];
			arr[j]--;
			j = (j + 1) % 3;
		}
		cout << endl;
	}
	return 0;
}
