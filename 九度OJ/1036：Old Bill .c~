#include <iostream>
using namespace std;

int main()
{
	int i, j;
	int n, x, y, z;
	int num;

	while(cin >> n)
	{
		cin >> x >> y >> z;
		num = 1000 * x + 100 * y + 10 * z;

		int resA, resB;
		resA = resB = -1;
		for(i = 9;i > 0;i--)
		{
			for(j = 9;j >= 0;j--)
				if ((num + i * 10000 + j) % n == 0)
				{
					resA = i;
					resB = j;
					break;
				}
			if (resA != -1)
				break;
		}

		if (resA != -1)
			cout << resA << " " << resB << " " << (num + 10000 * resA + resB) / n << endl;
		else
			cout << "0" << endl;
	}
	return 0;
}

