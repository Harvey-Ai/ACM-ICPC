#include <iostream>
#include <cstring>
using namespace std;

int flag[1000005];
int factorial[50];

int main()
{
	int a, i, j;

	factorial[0] = 1;
	for(i = 1;i < 11;i++)
		factorial[i] = factorial[i - 1] * i;

//	cout << "fa" << endl;
	memset(flag, 0, sizeof(flag));
	flag[0] = 1;
	for(i = 0;i < 11;i++)
		for(j = 100000;j >= factorial[i];j--)
			if (flag[j - factorial[i]])
				flag[j] = 1;
	
//	cout << "fa2" << endl;
			
	while(cin >> a)
	{
		if (flag[a])
			cout << "YES" << endl;
		else
			cout << "NO" << endl;
	}
	return 0;
}
