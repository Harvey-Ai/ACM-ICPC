#include <iostream>
using namespace std;

int main()
{
	int a;
	int i;

	while(cin >> a)
	{
		if (a < 2)
		{
			cout << "no" << endl;
			continue;
		}
		for(i = 2;i * i <= a;i++)
			if (a % i == 0)
				break;
		if (i * i <= a)
			cout << "no" << endl;
		else
			cout << "yes" << endl;
	}
	return 0;
}
