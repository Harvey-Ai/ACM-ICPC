#include <iostream>
#include <cstdio>
using namespace std;

int main()
{
	char str[1000];
	int i, j;
	char ss[1000];

	while(cin.getline(str, 1000, '\n'))
	{
		char c = getchar();
		getchar();

		for(j = 0, i = 0;str[i] != '\0';i++)
			if (str[i] != c)
				ss[j++] = str[i];
		ss[j] = '\0';
		cout << ss << endl;
	}
	return 0;
}

