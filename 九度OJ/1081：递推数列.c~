#include <iostream>
using namespace std;

int flag[10005];
int main()
{
	int a0, a1, p, q, k;

	memset(flag, -1, sizeof(flag));
	
	cin >> a0 >> a1 >> p >> q >> k;

	flag[a0] = a0;
	flag[a1] = a1;
	res[0] = a0;
	res[1] = a1;

	a2 = p * a1 + q * a0;
	circle = 1;
	for(i = 2;i <= k && flag[a2] != -1;++)
	{
		res[i] = a2;
		a0 = a1;
		a1 = a2;
		a2 = p * a1 + q * a0;
	}


}
