#include <stdio.h>

int main()
{
	int n;
	int i;
	int res;
	int j, a;
	
	while(scanf("%d", &n)!=EOF)
	{
		if (n == 0)
			break;
		for(i = 0;i < n;i++)
		{
			res = 0;
			scanf("%d", &a);
			for(j = 1;j * j < a;j++)
				if (a % j == 0)
					res += 2;
			if (j * j == a)
				res++;
			printf("%d\n", res);
		}
	}
	return 0;
}
