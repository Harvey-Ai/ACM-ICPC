#include <iostream>
using namespace std;

// 2 * x + 4 * y = n
int main()
{
	int ncase;
	int n;
	cin >> ncase;
	while(ncase--)
	{
		cin >> n;
		if (n % 2)
		{
			cout << "0 0" << endl;
			continue;
		}
		else
		{
			cout << n / 4 + (n % 4) / 2 << " " << n / 2 << endl;
		}
	}
	return 0;
}
