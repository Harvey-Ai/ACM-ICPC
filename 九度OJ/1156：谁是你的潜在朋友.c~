#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	int n, m;
	int i;
	int book[3000];
	int arr[2000];
	while(cin >> n >> m)
	{
		memset(book, 0, sizeof(book));
		for(i = 0;i < n;i++)
		{
			cin >> arr[i];
			book[arr[i]]++;
		}
		for(i = 0;i < n;i++)
			if (book[arr[i]] - 1)
				cout << book[arr[i]] - 1 << endl;
			else
				cout << "BeiJu" << endl;
	}
	return 0;
}
