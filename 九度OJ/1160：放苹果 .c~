#include <iostream>
using namespace std;

//S(n,n) = S(n,1) = 1,
//S(n,k) = S(n-1,k-1) + kS(n-1,k).
int st[15][15];

void initS()
{
	memset(st, 0, sizeof(st));
	//st[0][0] = 1;
	int i, j;

	for(i = 1;i < 11;i++)
		for(j = 1;j <= i;j++)
		{
			if (i == j || j == 1)
				st[i][j] = 1;
			else
			//	6 2				6 1				2 * 6 2
				st[i][j] = st[i - 1][j - 1] + j * st[i - 1][j];
		}
}

int main()
{
	int m, n, ncase;
	int i;
	cin >> ncase;

	initS();
	cout << st[6][2] << endl;
	while(ncase--)
	{
		cin >> n >> m;
		int res = 0;
		for(i = 1;i <= m;i++)
		{
			cout << st[n][i] << endl;
			res += st[n][i];
		}
		cout << res << endl;
	}
	return 0;
}
