#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
	int i;
	int arr[10000];
	int n;

	while(cin >> n)
	{
		for(i = 0;i < n;i++)
			cin >> arr[i];
		sort(&arr[0], &arr[n]);

		cout << arr[n-1] << endl;
		if (n == 1)
			cout << "-1" << endl;
		for(i = 0;i < n - 1;i++)
		{
			if (i > 0)
				cout << " ";
			cout << arr[i];
		}
		if (n != 1)
			cout << endl;
	}
	return 0;
}
