#include <iostream>
using namespace std;

#define pi 3.1415926

int main()
{
	int n;
	int i;
	double x, y, area;
	
	cin >> n;
	for(i = 1;i <= n;i++)
	{
		cin >> x >> y;
		area = (x * x + y * y) * pi / 2;
		int res = (int)(area / 50);
		if (res * 50 < area)
			res++;
		cout << "Property " << i << ": This property will begin eroding in year " << res << "." << endl;
	}
	cout << "END OF OUTPUT." << endl;
	return 0;
}
