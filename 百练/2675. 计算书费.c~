#include <stdio.h>

double price[10] = {28.9, 32.7, 45.6, 78, 35, 86.2, 27.8, 43, 56, 65};

int main()
{
	int i, ncase;
	double sum;
	int num[100];

	scanf("%d", &ncase);
	while(ncase--)
	{
		sum = 0;
		for(i = 0;i < 10;i++)
		{
			scanf("%d", &num[i]);
			sum += num[i] * price[i];
		}
		printf("%.2f\n", sum);
	}
	return 0;
}
