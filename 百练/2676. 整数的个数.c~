#include <iostream>
using namespace std;

int main()
{
	int arr[1000];
	int i, n;
	cin >> n;
	for(i = 0;i < n;i++)
		cin >> arr[i];

	int a = 0, b = 0, c = 0; 
	for(i = 0;i < n;i++)
		if (arr[i] == 1)
			a++;
		else if (arr[i] == 5)
			b++;
		else if (arr[i] == 10)
			c++;
	cout << a << endl << b << endl << c << endl;
	return 0;
}
