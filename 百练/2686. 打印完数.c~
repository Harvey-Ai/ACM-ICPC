#include <iostream>
using namespace std;

int main()
{
	int i, j;
	int sum;
	int factor[1001];
	int facNum;

	for(i = 1;i < 1001;i++)
	{
		sum = 0;
		facNum = 0;
		for(j = 1;j < i;j++)
			if (i % j == 0)
			{
				factor[facNum] = j;
				facNum++;
				sum += j;
			}
		if (sum == i)
		{
			cout <<i <<" its factors are ";
			cout << factor[0];
			for(j = 1;j < facNum;j++)
				cout << "," << factor[j];
			cout << endl;
		}
	}
	return 0;
}
