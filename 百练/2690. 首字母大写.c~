#include <iostream>
using namespace std;

int main()
{
	char str[1000];
	cin.getline(str, 100, '\n');

	int flag = 0;
	for(int i = 0;str[i] != '\0';i++)
		if (str[i] != ' ' && str[i] != '\t' && str[i] != '\r' && str[i] != '\n')
		{
			if (!flag)
			{
				if (str[i] >= 'a' && str[i] <= 'z')
					str[i] = str[i] - 'a' + 'A';
				flag = 1;
			}
		}
		else
			flag = 0;
	cout << str << endl;
	return 0;
}
