#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	int n;
	int a, b;
	char str[100];
	cin >> n;
	while(n--)
	{
		cin >> a;
		cin >> str;
		cin >> b;
		if (strcmp(str, "mul") == 0)
			cout << a * b << endl;
		else if (strcmp(str, "div") == 0)
			cout << a / b << endl;
		else if (strcmp(str, "add") == 0)
			cout << a + b << endl;
		else if (strcmp(str, "sub") == 0)
			cout << a - b << endl;
		else if (strcmp(str, "mod") == 0)
			cout << a % b << endl;
	}
	return 0;
}
