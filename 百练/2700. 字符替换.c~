#include <iostream>
using namespace std;

int main()
{
	int ncase;
	int i;
	char old, neww;
	char str[10000];
	cin >> ncase;

	while(ncase--)
	{
		cin >> str;
		cin >> old >> neww;

		for(int i = 0;str[i] != '\0';i++)
			if (str[i] == old)
				str[i] = neww;
		cout << str << endl;
	}
	return 0;
}
