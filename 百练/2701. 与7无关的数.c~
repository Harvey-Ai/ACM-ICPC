#include <stdio.h>
#include <string.h>

int main()
{
	int i, j;
	int res;
	int arr[105];
	int n;

	memset(arr, 0, sizeof(arr));
	for (i = 0;i < 10;i++)
	{
		if (i == 7)
			continue;
		for (j = 0;j < 10;j++)
		{
			if (j == 7)
				continue;
			if ((i * 10 + j) % 7 ==0)
				continue;
			arr[i * 10 + j] = 1;
		}
	}

	while(scanf("%d", &n) != EOF)
	{
		res = 0;
		for(i = 0;i <= n;i++)
			if (arr[i])
				res += i * i;
		printf("%d\n", res);
	}
}
