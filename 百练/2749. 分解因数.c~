#include <iostream>
using namespace std;

int f(int num, int max)
{
	if (num == 1)
		return 1;
	int res = 0;
	for(int i = 2;i <= max;i++)
		if (num % i == 0)
		{
		//	cout << n << endl;
			res += f(num / i, i);
		}
	return res;
}

int main()
{
	int ncase;

	cin >> ncase;
	while(ncase--)
	{
		int a;
		cin >> a;
		cout << f(a, a) << endl;
	}
	return 0;
}
