#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	char str[1000];
	int i;
	while(cin >> str)
	{
		if (strcmp(str, "ENDOFINPUT") == 0)
			break;
		int flag = 0;
		while(cin >> str)
		{
			if (flag)
				cout << " ";
			else
				flag = 1;
			if (strcmp(str, "END") == 0)
				break;
			for(i = 0;str[i] != '\0';i++)
			{
				if (str[i] >= 'A' && str[i] <= 'Z')
				{
					char newOne = (char)(str[i] - 5);
					if (newOne < 'A')
						newOne += 26;
					cout << newOne;
				}
				else
					cout << str[i];
			}
		}
		cout << endl;
	}
	return 0;
}
