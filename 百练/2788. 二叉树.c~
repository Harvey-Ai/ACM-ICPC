#include <iostream>
using namespace std;

int main()
{
	int m, n;
	int deep1, deep2;
	int res;
	int temp;

	while(cin >> m >> n, m && n)
	{
		deep1 = deep2 = m;
		res = 0;
		temp = 1;
		while(deep1 <= n)
		{
			res += temp;
			temp *= 2;
			deep2 = deep2 * 2 + 1;
			deep1 *= 2;
		}
		deep2 = (deep2 - 1) / 2;
	//	cout << deep2 << endl;
		if (deep2 > n)	res -= deep2 - n;
		cout << res << endl;
	}
	return 0;
}
