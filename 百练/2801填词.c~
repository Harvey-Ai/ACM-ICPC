#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	int m, n, p;
	int i, j;
	char g[100][100];
	int flag[100];
	cin >> n >> m >> p;

	for(i = 0;i < n;i++)
		cin >> g[i];

	memset(flag, 0, sizeof(flag));
	for(i = 0;i < n;i++)
		for(j = 0;j < m;j++)
			flag[(int)(g[i][j] - 'A')]++;

	for(i = 0;i < p;i++)
	{
		cin >> g[0];
		for(j = 0;g[0][j] != '\0';j++)
			flag[g[0][j] - 'A']--;
	}

	for(i = 0;i < 26;i++)
		while(flag[i])
		{
			cout << (char)(i + 'A');
			flag[i]--;
		}
	cout << endl;
	return 0;
}
