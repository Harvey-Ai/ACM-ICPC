#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	int arr[100];
	int flag[300];
	int cnt;
	int i, n;
	while(cin >> arr[0], arr[0] != -1)
	{
		i = 1;
		while(cin >> arr[i], arr[i])
			i++;

		n = i;
		memset(flag, 0, sizeof(flag));
		for(i = 0;i < n;i++)
			flag[arr[i]] = 1;

		cnt = 0;
		for(i = 1;i < 101;i++)
			if(flag[i] && flag[i * 2])
				cnt++;
		cout << cnt << endl;
	}

	return 0;
}
