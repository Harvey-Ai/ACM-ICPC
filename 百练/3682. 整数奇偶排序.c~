#include <iostream>
#include <algorithm>
using namespace std;

bool cmp(const int a, const int b)
{
	return a < b ? 0 : 1;
}

int main()
{
	int num1, num2;
	int a[100];
	int odd[100], even[100];
	int i;

	while(cin >> a[0] >> a[1] >> a[2] >> a[3] >> a[4] >> a[5] >> a[6] >> a[7] >> a[8] >> a[9])
	{
		num1 = num2 = 0;
		for(i = 0;i < 10;i++)
			if (a[i] % 2)
				odd[num1++] = a[i];
			else
				even[num2++] = a[i];

		sort(&odd[0], &odd[num1], cmp);
		sort(&even[0], &even[num2]);
		for(i = 0;i < num1;i++)
			cout << odd[i] << " ";
		for(i = 0;i < num2;i++)
			cout << even[i] << " ";
		cout << endl;
	}
	return 0;
}
