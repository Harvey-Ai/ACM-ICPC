#include <stdio.h>

void convert(int n, char *str)
{
	int i = 0;
	while(n)
	{
		str[i++] = (n & 0x01) + '0';
		n>>=1;
	}
	while(i < 16)
		str[i++] = '0';
	str[16] = '\0';
}
int main()
{
	int i, j, len, a, b;
	char str1[40], str2[40];
	int ncase;
	scanf("%d", &ncase);
	while(ncase--)
	{
		scanf("%d %d", &a, &b);
		convert(a, str1);
		convert(b, str2);

		len = 16;
		for(i = len - 1;i >= 0;i--)
		{
			for(j = len - 1;j >= 0;j--)
				if (str2[j] != str1[(i + 1 + j)%len])
					break;
			if (j < 0)
				break;
		}
		if (i >= 0)
			printf("YES\n");
		else
			printf("NO\n");
	}
}
