#include <stdio.h>
#include <string.h>

int main()
{
	char str[205], sub[205];
	int dp[205][205];
	int i, j;
	int len1, len2;
	int max, temp;

	while(scanf("%s %s", sub, str) != EOF)
	{
		len1 = strlen(sub);
		len2 = strlen(str);

		for(i = 0;i < len1;i ++)
			for(j = 0;j < len2;j++)
			{
				max = 0;

				if (i > 0 && max < dp[i - 1][j])
					max = dp[i - 1][j];
				if (j > 0 && max < dp[i][j - 1])
					max = dp[i][j - 1];
				if (sub[i] == str[j])
				{
					if (i > 0 && j > 0)
						temp = dp[i - 1][j - 1] + 1;
					else
						temp = 1;
					if (temp > max)
						max =  temp;
				}
				dp[i][j] = max;
			}

		if (dp[len1 - 1][len2 - 1] >= len1)
			printf("Yes\n");
		else
			printf("No\n");
	}
	return 0;
}
