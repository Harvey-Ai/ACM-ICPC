#include <stdio.h>

int monthDay(int n)
{
	if (n == 2)
		return 29;
	if (n <= 7)
	{
		if (n % 2 == 1)
			return 31;
		else
			return 30;
	}
	else
	{
		if (n % 2 == 0)
			return 31;
		else
			return 30;
	}
}

int main()
{
	int i;
	int ansNum;
	int ans[100];
	int wd;

	while(scanf("%d", &wd) != EOF)
	{
		wd--;
		ansNum = 0;
		for(i = 1;i < 13;i++)
		{
			if ((wd + 13 - 1) % 7 == 4)
				ans[ansNum++] = i;
			wd = (wd + monthDay(i)) % 7;
		}
		for(i = 0;i < ansNum;i++)
			printf("%d ", ans[i]);
		putchar('\n');
	}
	return 0;
}
