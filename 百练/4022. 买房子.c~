#include <stdio.h>

int main()
{
	double n, k;
	double year;
	double mm;

	while(scanf("%lf %lf", &n, &k) != EOF)
	{
		mm = 200;
		year = 1;
		while(year <= 20)
		{
			if (mm <= n * year)
				break;
			else
				mm *= (1 + k * 1.0 / 100);
			year++;
		}
		if (year <= 20)
			printf("%.0lf\n", year);
		else
			printf("Impossible\n");
	}
	return 0;
}
