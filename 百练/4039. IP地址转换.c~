#include <iostream>
#include <cstring>
using namespace std;

int main()
{
	char str[100];
	char res[100];
	int pos, num;
	int i, k, j;
	while(cin>>str)
	{
		if (strcmp(str, "-1") == 0)
			break;

		k = 0;
		pos = 0;
		for(i = 0;str[i] != '\0';i++)
			if (str[i] == '.')
			{
				num = 0;
				int temp = 1;
				for(j = i - 1;j >= pos;j--)
				{
					num += (str[j] - '0') * temp;
					temp *= 10;
				}
				//cout << num << endl;
				for(j = 0;j < 8;j++)
				{
					if (num & 128)
						res[k++] = '1';
					else
						res[k++] = '0';
					num <<= 1;
				}
				pos = i + 1;
			}
		int temp = 1;
		num = 0;
		for(j = i - 1;j >= pos;j--)
		{
			num += (str[j] - '0') * temp;
			temp *= 10;
		}
		for(j = 0;j < 8;j++)
		{
			if (num & 128)
				res[k++] = '1';
			else
				res[k++] = '0';
			num <<= 1;
		}
		res[k] = '\0';
		cout << res << endl;
	}
	return 0;
}
