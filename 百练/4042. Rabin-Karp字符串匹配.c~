#include <iostream>
using namespace std;

int main()
{
	int ncase;
	int s, q;
	int cnt;
	int i;
	char str[200];
	char res[200][200];
	char *posB, *posE, *p;
	cin >> ncase;

	while(ncase--)
	{
		cin >> str;
		cin >> s >> q;
		posB = &str[0];posE = &str[s - 1];
		int sum = 0;
		cnt = 0;
		for(p = posB;p <= posE;p++)
			sum += *p - 'a' + 1;
		if (sum == q)
		{
			for(i = 0, p = posB;p <= posE;p++,i++)
				res[cnt][i] = *p;
			res[cnt++][i] = '\0';
		}
		posB++;posE++;
		for(;*posE != '\0';posE++, posB++)
		{
			sum -= *(posB - 1) - 'a' + 1;
			sum += *posE - 'a' + 1;
			//cout << *(posB - 1) << " " << *posE << " " << sum << endl;
			if (sum == q)
			{	
				for(i = 0, p = posB;p <= posE;p++,i++)
					res[cnt][i] = *p;
				res[cnt++][i] = '\0';
			}
		}
		cout << cnt << endl;
		for(i = 0;i < cnt;i++)
			cout << res[i] << endl;
	}
	return 0;
}
