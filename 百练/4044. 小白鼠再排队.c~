#include <iostream>
#include <algorithm>
using namespace std;

struct ms
{
	int weight;
	char color[100];
};

bool cmp(const struct ms a, const struct ms b)
{
	return a.weight - b.weight > 0 ? 0 : 1;
}

int main()
{
	int n;
	struct ms mouse[150];
	int i;
	cin >> n;
	for(i = 0;i < n;i++)
		cin >> mouse[i].weight >> mouse[i].color;

	sort(&mouse[0], &mouse[n], cmp);

	for(i = 0;i < n;i++)
		cout << mouse[i].color << endl;
	return 0;
}
