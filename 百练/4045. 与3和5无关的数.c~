#include <iostream>
#include <string>
using namespace std;

int main()
{
	int flag[400];
	int a, b;
	int res;
	int i, n;

	for(i = 1;i < 300;i ++)
	{
		flag[i] = 0;
		if (i % 3 == 0 || i % 5 == 0)
			flag[i] = 1;

		a = i;
		while(a)
		{
			if (a % 10 == 3 || a % 10 == 5)
				flag[i] = 1;
			a /= 10;
		}
	}

	cin >> n;
	res = 0;
	for(i = 1;i <= n;i++)
		if (!flag[i])
			res += i * i;
	cout<< res;

	return 0;
}
