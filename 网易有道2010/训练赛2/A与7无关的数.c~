#include <stdio.h>
#include <string.h>
#define N 101

int arr[N];
int main()
{
	int n;
	int i, j;
	int res;
	memset(arr, 0, sizeof(arr));
	for(i = 1;i * 7 < N;i++)
		arr[i * 7] = 1;

	for(i = 0;i < 10;i++)
		for(j = 0;j < 10;j++)
			if (i == 7 || j == 7)
				arr[i * 10 + j] = 1;

	scanf("%d", &n);

	res = 0;
	for(i = 1;i <= n;i++)
		if (arr[i] == 0)
			res += i * i;

	printf("%d\n", res);
	return 0;
}	
