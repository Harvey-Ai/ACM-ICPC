#include <stdio.h>
#include <stdlib.h>
#define N 105

int sum[N * N];

int cmp(const void *a, const void *b)
{
	return *((int *)a) - *((int *)b);
}
int Bsearch(int num, int n)
{
	int low = 0;
	int high = n - 1;
	int mid;

	while(low <= high)
	{
		mid = (high + low) / 2;
		if (num == sum[mid])
			return 1;
		else
		{
			if (sum[mid] > num)
				high = mid - 1;
			else
				low = mid + 1;
		}
	}
	return 0;
}
int main()
{
	int i, j, k, l, n;
	int ncase;
	int res;
	int arr[N];

	scanf("%d", &ncase);

	while(ncase--)
	{
		scanf("%d", &n);
		for(i = 0;i < n;i++)
			scanf("%d", &arr[i]);

		k = 0;
		for(i = 0;i < n;i++)
			for(j = i + 1;j < n;j++)
				if (i != j)
				{
					if (arr[i] == 0 || arr[j] == 0)
					{
						for(l = 0;l < n;l++)
							if (arr[l] == arr[j] + arr[i] && l != j && l != i)
								break;
						if (l < n)
							sum[k++] = arr[i] + arr[j];
					}
					else
						sum[k++] = arr[i] + arr[j];
				}

		qsort(sum, k, sizeof(sum[0]), cmp);

		res = 0;
		for(i = 0;i < n;i++)
			if (Bsearch(arr[i], k))
				res++;
		printf("%d\n", res);
	}
	return 0;
}
